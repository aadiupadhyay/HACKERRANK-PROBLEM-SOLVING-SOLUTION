#include<stdio.h>
void verybigsum(long int a[],int n)
{
    long int s=0;
    for(int i=0;i<n;i++)
    s=s+a[i];
    printf("%ld",s);
}

int main()
    {
        int i,n;
        long int a[10];
        scanf("%d",&n);
        for(i=0;i<n;i++)
        scanf("%ld",&a[i]);
        verybigsum(a,n);
    }
