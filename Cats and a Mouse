#include<stdio.h>
int main(){
    int x,y,z,d,e,t;
    scanf("%d",&t);
    while(t>0){
        scanf("%d%d%d",&x ,&y ,&z);
        if(x>z)
        d=x-z;
        else
        d=z-x;
        if(y>z)
        e=y-z;
        else
        e=z-y;
        if(e>d)
        printf("Cat A\n");
        else if(d>e)
        printf("Cat B\n");
        else
        printf("Mouse C\n");
        t-=1;
    }
    return 0;
}
