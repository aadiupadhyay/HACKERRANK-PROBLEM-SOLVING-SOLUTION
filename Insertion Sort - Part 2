#include<stdio.h>
int min(int a[],int n){
    int i,p=a[n-1],min=a[0];
    for(i=1;i<n;i++){
        if(a[i]<min)
        min=a[i];
    }
    return min;
}
void print(int a[],int n){
    int i;
    for(i=0;i<n;i++)
    printf("%d ",a[i]);
    printf("\n");
}
int main(){
    int n,i,j;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++){
        scanf("%d",&a[i] );
    }
    for(i=0;i<n-1;i++){
        if(a[i]<a[i+1]){
            print(a,n);
            continue;
            
        }
        
        else{
            int k=i+1;
            for(j=i;j>=0;j--){
                while(a[j]>a[k]){
                    int temp=a[k];
                    a[k]=a[j];
                    a[j]=temp;
                    k--;
                    
                }
            }
            print(a,n);    
        }
    }
}
