#include<stdio.h>
void plusminus(int a[],int n)
{
   int i,d=0,e=0,c=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>0)
        c+=1;
        else if(a[i]<0)
        d+=1;
        else 
        e+=1;
    }
    printf("%f\n",(float)c/n);
    printf("%f\n",(float)d/n);
    printf("%f",(float)e/n);
}
int  main()
{
    int i,n;
    int a[100];
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    plusminus(a,n);

}
